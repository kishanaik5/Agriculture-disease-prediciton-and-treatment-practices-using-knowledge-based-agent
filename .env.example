# Database - PostgreSQL
# Replace localhost if using a remote server
POSTGRES_SERVER=localhost
# Default user is often postgres
POSTGRES_USER=postgres
# Your DB Password
POSTGRES_PASSWORD=your_password
# Database Name
POSTGRES_DB=kisaan_sampurna
POSTGRES_PORT=5432

# Gemini AI - Google Generative AI
# Get key from AI Studio
GEMINI_API_KEY=your_gemini_api_key
# Models Config
GEMINI_MODEL_FLASH=gemini-2.5-flash
GEMINI_MODEL_PRO=gemini-2.5-pro
GEMINI_MODEL_BBOX=gemini-3-pro-preview

# AWS S3 - Storage
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key
AWS_REGION=us-east-1
S3_BUCKET_PRIVATE=your_private_bucket_name
S3_BUCKET_PUBLIC=your_public_bucket_name

# App Security
SECRET_KEY=change_this_secret_key_to_something_secure
ENV_MODE=dev
